return false;});$(".meetNowButton").bind('click',function(e){var startMeetingBlock=$(this).siblings('div.startMeeting');var spinner=$(this).siblings('.waitSpin');e.preventDefault();ajax_url="/meeting/user_status?user_id="+slideshare_object.user.id+"&ss_id="+slideshare_object.presentationId;startMeetingBlock.html("");$(this).attr('','').addClass("ctaNormal");spinner.show();$.ajax({url:ajax_url,complete:function(request,data){startMeetingBlock.html(request.responseText).slideDown();spinner.hide();$(".cancelMeetNow",startMeetingBlock).show();}});return false;});});})(jQuery);