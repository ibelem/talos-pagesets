function movebrk(){
if(stop){
_stpos = 0;}
getObj('bnewstickerplaceholder').style.left=(_stpos - 2)+'px';	
if(getObj('bnewstickerplaceholder').style.left == (_imgwt*-1)+"px"){
_stpos = 348;}
_stpos = _stpos - 1;
if(!stop){
if(is_chrome){
setTimeout('movebrk()',10);
}else {
setTimeout('movebrk()',15);
}}}
function bnews(refreshtime){
try{
var breakhttp=false;
breakhttp = window.XMLHttpRequest?new XMLHttpRequest():new ActiveXObject("Microsoft.XMLHTTP");
if (breakhttp){
breakhttp.onreadystatechange=function(){
if (breakhttp.readyState==4){
if (breakhttp.status==200){
var bstr = breakhttp.responseText;
if(bstr == ""){
if(getObj("newsbrk").style.display == "inline-block"){
getObj("newsbrk").style.display = "none";
}
}else{
getObj('bnewstickerplaceholder').innerHTML=bstr;
getObj("newsbrk").style.display="inline-block";
_imgwt = getObj("bnewstickerplaceholder").offsetWidth;
if(_imgwt >= 345 && moving == false){
moving = true;
stop = false;
movebrk();
}else if(_imgwt < 345 && moving == true){
stop = true;
moving = false;
}}}}}
void("GET",'http://www.indiatimes.com/inbetabreakingnews/bnews.htm',true);
breakhttp.send(null);
}
}catch (e){breakhttp = false;}	
setTimeout("bnews("+refreshtime+")",refreshtime);
}
