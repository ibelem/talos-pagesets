function chkDiv(e){

var targ;
if (!e) var e = window.event;
if (e.target) targ = e.target;
else if (e.srcElement) targ = e.srcElement;
var objDiv=document.getElementById("srchcmb2");
var arrimg=document.getElementById("mnuarr");
if (targ.nodeName != "HTML")
{
while (targ.nodeName != "BODY")
{
if (targ.id && targ.id == 'srchcmb2')
{
if(objDiv.style.display=="none")
{objDiv.style.display="block";

}
else
{
objDiv.style.display="none";}
return;
}
else
{
if ((targ.id && targ.id == 'srchsel2') || targ.id=='secdiv')
{
if(objDiv.style.display=="none")
{
objDiv.style.display="block";

arrimg.className='rotimg2';

}
else
{
objDiv.style.display="none";
arrimg.className='rotimg1';
}
return;
}

else
targ = targ.parentNode;
}
}
}
objDiv.style.display="none";arrimg.className='rotimg1';
closeSel(e)
}


