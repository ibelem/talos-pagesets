			}else{
				
				eval("this.c.style."+this.p+"=this.getNCP(d)+'px'");
			}
		};		
		this.getNP=function(d){if(d=='l'){return (this.getCP()-(this.s*this.n));}else{return(this.getCP()+(this.s*this.n));}};
